{
  "name": "aurora-hive",
  "productName": "AuroraHive",
  "version": "0.0.2",
  "description": "The Auroracoin (AUR) hive wallet in your menubar",
  "license": "MIT",
  "repository": "herrhelms/aurora-hive",
  "author": {
    "name": "herrhelms"
  },
  "env": {
    "showDockIcon": false
  },
  "engines": {
    "node": ">=4"
  },
  "electronVersion": "1.3.4",
  "scripts": {
    "start": "electron . &",
    "build-mac": "electron-packager . --platform=darwin --icon=Icon --arch=x64 --out=dist AuroraHive"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "menubar",
    "crypto",
    "auroracoin",
    "AUR"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/herrhelms/aurora-hive.git"
  },
  "bugs": {
    "url": "https://github.com/herrhelms/aurora-hive/issues"
  },
  "dependencies": {
    "electron-debug": "^1.0.1",
    "menubar": "^5.1.0"
  },
  "devDependencies": {
    "electron-packager": "^7.7.0",
    "electron": "^1.3.4"
  }
}
